package classes;

public class DataTeste {

	public static void main(String[] args) {
		
		Data d1 = new Data();
		d1.dia = 03;
		d1.mes = 07;
		d1.ano = 1994;
		
		var d2 = new Data();
		d2.dia = 16;
		d2.mes = 07;
		d2.ano = 1997;
		
		System.out.println(d2.dia);
		
		System.out.printf("A data é dia %d do mês %d do ano de %d", d1.dia, d1.mes, d1.ano);
		
		//System.out.printf("A data é % de % de %", d2.dia, d2.mes, d2.ano);
		
	}
}
